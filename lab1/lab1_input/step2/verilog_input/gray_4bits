#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x16abe50 .scope module, "TB2" "TB2" 2 14;
 .timescale -9 -12;
P_0x16be9b8 .param/l "N" 2 15, +C4<01000>;
P_0x16be9e0 .param/l "ONE" 2 21, C4<1>;
v0x16e5d60_0 .var "clk", 0 0;
v0x16e5e00_0 .net "leds", 7 0, L_0x16e5f60; 1 drivers
v0x16e5eb0_0 .var "rst", 0 0;
S_0x16aad60 .scope module, "DUT" "gray_Nbits" 2 42, 3 8, S_0x16abe50;
 .timescale 0 0;
P_0x16aae58 .param/l "N" 3 9, +C4<01000>;
P_0x16aae80 .param/l "SIZE" 3 10, +C4<01001>;
P_0x16aaea8 .param/l "Zeros" 3 11, C4<000000000>;
v0x16aaee0_0 .net "clk", 0 0, v0x16e5d60_0; 1 drivers
v0x16e57c0_0 .net "clk_en", 0 0, C4<1>; 1 drivers
v0x16e5860_0 .alias "gray_out", 8 1, v0x16e5e00_0;
v0x16e5900_0 .var/i "i", 31 0;
v0x16e59b0_0 .var "isZero", 0 0;
v0x16e5a50_0 .var/i "j", 31 0;
v0x16e5b30_0 .net "rst", 0 0, v0x16e5eb0_0; 1 drivers
v0x16e5bd0_0 .var "state", 8 0;
v0x16e5cc0_0 .var "toggle", 8 0;
E_0x16ac080 .event edge, v0x16e5bd0_0;
E_0x16aba20/0 .event edge, v0x16e5b30_0;
E_0x16aba20/1 .event posedge, v0x16aaee0_0;
E_0x16aba20 .event/or E_0x16aba20/0, E_0x16aba20/1;
L_0x16e5f60 .part v0x16e5bd0_0, 1, 8;
    .scope S_0x16aad60;
T_0 ;
    %wait E_0x16aba20;
    %load/v 8, v0x16e5b30_0, 1;
    %jmp/0xz  T_0.0, 8;
    %movi 8, 1, 9;
    %ix/load 0, 9, 0;
    %assign/v0 v0x16e5bd0_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x16e5bd0_0, 9;
    %load/v 17, v0x16e5cc0_0, 9;
    %xor 8, 17, 9;
    %ix/load 0, 9, 0;
    %assign/v0 v0x16e5bd0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x16aad60;
T_1 ;
    %wait E_0x16ac080;
    %ix/load 0, 1, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v0x16e5cc0_0, 0, 1;
    %load/v 8, v0x16e5bd0_0, 1; Only need 1 of 9 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 1, 0;
    %assign/v0/x1 v0x16e5cc0_0, 0, 8;
    %movi 8, 2, 32;
    %set/v v0x16e5900_0, 8, 32;
T_1.0 ;
    %load/v 8, v0x16e5900_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_1.1, 5;
    %set/v v0x16e59b0_0, 1, 1;
    %load/v 8, v0x16e5900_0, 32;
    %mov 40, 39, 1;
    %subi 8, 2, 33;
    %set/v v0x16e5a50_0, 8, 32;
T_1.2 ;
    %load/v 8, v0x16e5a50_0, 32;
    %cmp/s 0, 8, 32;
    %or 5, 4, 1;
    %jmp/0xz T_1.3, 5;
    %load/v 8, v0x16e59b0_0, 1;
    %ix/getv/s 1, v0x16e5a50_0;
    %jmp/1 T_1.4, 4;
    %load/x1p 9, v0x16e5bd0_0, 1;
    %jmp T_1.5;
T_1.4 ;
    %mov 9, 2, 1;
T_1.5 ;
; Save base=9 wid=1 in lookaside.
    %inv 9, 1;
    %and 8, 9, 1;
    %set/v v0x16e59b0_0, 8, 1;
    %load/v 8, v0x16e5a50_0, 32;
    %subi 8, 1, 32;
    %set/v v0x16e5a50_0, 8, 32;
    %jmp T_1.2;
T_1.3 ;
    %load/v 8, v0x16e5900_0, 32;
    %subi 8, 1, 32;
    %ix/get/s 1, 8, 32;
    %jmp/1 T_1.6, 4;
    %load/x1p 8, v0x16e5bd0_0, 1;
    %jmp T_1.7;
T_1.6 ;
    %mov 8, 2, 1;
T_1.7 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v0x16e59b0_0, 1;
    %and 8, 9, 1;
    %ix/getv/s 1, v0x16e5900_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 1, 0;
    %assign/v0/x1 v0x16e5cc0_0, 0, 8;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x16e5900_0, 32;
    %set/v v0x16e5900_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %load/v 8, v0x16e5bd0_0, 7; Only need 7 of 9 bits
; Save base=8 wid=7 in lookaside.
    %or/r 8, 8, 7;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 8, 0;
    %assign/v0/x1 v0x16e5cc0_0, 0, 8;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x16abe50;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "TB2.vcd";
    %vpi_call 2 26 "$dumpvars", 1'sb0, S_0x16abe50;
    %set/v v0x16e5d60_0, 0, 1;
    %set/v v0x16e5eb0_0, 1, 1;
    %delay 5000, 0;
    %set/v v0x16e5eb0_0, 0, 1;
    %delay 200000, 0;
    %vpi_call 2 33 "$finish";
    %end;
    .thread T_2;
    .scope S_0x16abe50;
T_3 ;
    %delay 5000, 0;
    %load/v 8, v0x16e5d60_0, 1;
    %inv 8, 1;
    %set/v v0x16e5d60_0, 8, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "TB2_Gray.v";
    "GrayCounter_Nbits.v";
