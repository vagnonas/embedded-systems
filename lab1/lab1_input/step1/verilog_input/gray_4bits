    %cmpi/u 8, 10, 4;
    %jmp/1 T_0.14, 6;
    %cmpi/u 8, 11, 4;
    %jmp/1 T_0.15, 6;
    %cmpi/u 8, 9, 4;
    %jmp/1 T_0.16, 6;
    %cmpi/u 8, 8, 4;
    %jmp/1 T_0.17, 6;
    %jmp T_0.18;
