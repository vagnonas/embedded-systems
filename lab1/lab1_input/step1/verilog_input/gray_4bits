:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x21a8610 .scope module, "TB1" "TB1" 2 14;
 .timescale -9 -12;
P_0x21d1988 .param/l "ONE" 2 21, C4<1>;
v0x21e1d80_0 .var "clk", 0 0;
v0x21e1e50_0 .net "leds", 3 0, v0x21e1ce0_0; 1 drivers
v0x21e1f00_0 .var "rst", 0 0;
S_0x21a9440 .scope module, "DUT" "gray_4bits" 2 42, 3 28, S_0x21a8610;
